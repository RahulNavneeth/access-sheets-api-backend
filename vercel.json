{
  "version": 2,

  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "headers": {
        "key": "Access-Control-Allow-Headers",
        "value": "Origin, X-Requested-With, Content-Type, Accept, Authorization, X-CSRF-Token"
      }
    }
  ]
}
